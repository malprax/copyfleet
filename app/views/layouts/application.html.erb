<!DOCTYPE html>
<html>
<head>
  <title>Copyfleet</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'shared/header' %>
<div class="container">
    <div class="row">
        <%= render 'shared/left_menu' %>
        <div class="span7"><%= yield %></div>
        <div class="span3">
          <%= yield(:actions_panel) %>
          <%= yiled(:filters) %>
        </div>
    </div>
</div>

<%= javascript_include_tag 'application' %>
</body>
</html>
